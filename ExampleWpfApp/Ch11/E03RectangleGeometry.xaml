<Page x:Class="ExampleWpfApp.Ch11.E03RectangleGeometry"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExampleWpfApp.Ch11"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="RectangleGeometry">
    <Grid>
        <Path Stroke="Blue" StrokeThickness="5">
            <Path.Data>
                <GeometryGroup>
                    <RectangleGeometry Rect="450,20,50,30" />
                    <RectangleGeometry Rect="450,70,50,30" />
                    <RectangleGeometry Rect="450,120,50,30" />
                </GeometryGroup>
            </Path.Data>
        </Path>
        <Canvas Name="canvas1"/>
    </Grid>
</Page>
